	box-shadow:	        0 0 10px rgba(0,0,0,0.8);
	text-align: center;
}
.popup:before {
	content:"";
	background:url(images/lines.png);
	position: absolute;
	top: -3px;
	left: -3px;
	bottom: -3px;
	right: -3px;
	-o-transition:      all 0.2s linear;
	-moz-transition:    all 0.2s linear;
	-webkit-transition: all 0.2s linear;
	transition:         all 0.2s linear;
}
