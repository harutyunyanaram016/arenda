    float: left;
}

div.close {
    background: #1e1e1e;
    width: 30px;
    height: 30px;
    line-height: 30px;
    text-align: center;
    font-size: 15px;
    color: #fff;
    position: absolute;
    top: 20px;
    right: 20px;
    z-index: 2;
    cursor: pointer;
}

.popup p {
	font-size: 25px;
	margin: 20px 0px 10px 0px;
}
.popup input[type="text"] {
    background: #1e1e1e;
    width: 82%;
    margin: 20px 5% 15px;
    padding: 15px 4%;
	color: #fff;
	font-size: 17px;
	font-family: 'days';
    float: left;
    clear: left;
	-o-transition:      all 0.2s linear;
	-moz-transition:    all 0.2s linear;
	-webkit-transition: all 0.2s linear;
	transition:         all 0.2s linear;
}
.popup input[type="text"]:focus {
   text-shadow: 0px 0px 4px #fff;
}
.popup input[type="submit"] {
    background: url(images/call.png);
    width: 274px;
    height: 95px;
    line-height: 95px;
    text-align: center;
    margin: 20px 0px -50px -135px;
    font-size: 25px;
    position: relative;
    left: 50%;
    cursor: pointer;
	font-family: 'days';
	color: #333;
    float: left;
	-moz-box-shadow: 	0 4px 15px rgba(0,0,0,0.5);
    -webkit-box-shadow: 0 4px 15px rgba(0,0,0,0.5);
    box-shadow: 		0 4px 15px rgba(0,0,0,0.5);
	-o-transition:      all 0.2s linear;
	-moz-transition:    all 0.2s linear;
	-webkit-transition: all 0.2s linear;
	transition:         all 0.2s linear;
}
.popup input[type="submit"]:hover {
	color: #1e1e1e;
}

