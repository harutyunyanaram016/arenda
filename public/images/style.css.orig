*{ 
	margin: 0;
	padding: 0;
	border: 0;
	border-collapse: collapse;
}

body {
	background: #e5e5e5;
	font-size: 14px;
	color: #1e1e1e;
	font-family: 'days';
}


a {
	text-decoration: none;
	color: #1e1e1e;
}

div.over {
	min-width: 1300px;
	width: 100%;
	overflow: hidden;
	position: relative;
	float: left;
}


div.center{
	min-width: 990px;
	max-width: 990px;
	margin: 0 auto;
}


div.header {
	position: absolute;
	top: 0;
	left: 0;
	right: 0;
	z-index: 6;
}

div.header,
div.main_block {
	max-height: 980px;
}

div.logo {
	font-size: 50px;
	color: #1e1e1e;
	position: absolute;
	top: 240px;
	left: 100px;
}
.logo a {
	color: #1e1e1e;
	float: left;
}

div.slogan {
	font-size: 15px;
	color: #fff;
	position: relative;
	float: left;
	clear: left;
}
.slogan span {
	margin: 0px 10px 0px 0px;
}
.slogan:before {
	content:"";
	background: #1e1e1e;
	width: 800px;
	height: 26px;
	position: absolute;
	top: -3px;
	right: 0;
	z-index: -1;
}


div.main_menu {
	position: absolute;
	top: 400px;
	left: 100px;
}
ul {
	list-style-type: none;
}
.main_menu li {
	margin: 0px 0px 10px 0px;
	display: block;
	float: left;
	clear: left;
}
.main_menu li a { 
    font-size: 20px;
	color: #1e1e1e;
	display: block;
	position: relative;
	text-decoration: none;
	float: left;
	-o-transition:      all 0.2s linear;
	-moz-transition:    all 0.2s linear;
	-webkit-transition: all 0.2s linear;
	transition:         all 0.2s linear;
}
.main_menu li a:hover { 
	color: #fff;
}
.main_menu li a:before { 
	content:"";
    position: absolute;
	width: 400px;
	border-bottom: 1px solid #1e1e1e;
	bottom: 0;
	right: 0;
	z-index: -1;
	-o-transition:      all 0.2s linear;
	-moz-transition:    all 0.2s linear;
	-webkit-transition: all 0.2s linear;
	transition:         all 0.2s linear;
}

.main_menu li a:after { 
	content:"";
    position: absolute;
	height: 30px;
	bottom: -3px;
	right: -10px;
	left: -100px;
	z-index: -1;
	-o-transition:      all 0.2s linear;
	-moz-transition:    all 0.2s linear;
	-webkit-transition: all 0.2s linear;
	transition:         all 0.2s linear;
}
.main_menu li a:hover:after { 
	background:#1e1e1e;
}



div.telefon {
	text-align: right;
	position: absolute;
	top: 110px;
	right: 100px;
	font-size: 30px;
}
.telefon a {
	font-size: 18px;
}




div.call {
	background:url(images/call.png);
	width: 274px;
	height: 95px;
	line-height: 95px;
	text-align: center;
	margin: 0px 0px 0px -50px;
	font-size: 25px;
	position: absolute;
	left: 50%;
	bottom: 80px;
	cursor:pointer;
}
.header .call {
	opacity: 0;
	-o-transition:      all 0.2s ease;
	-moz-transition:    all 0.2s ease;
	-webkit-transition: all 0.2s ease;
	transition:         all 0.2s ease;
}
.header .call:hover {
	opacity: 1;
}
.call:before {
	content:"";
	position: absolute;
	top: 3px;
	left: 3px;
	bottom: 3px;
	right: 3px;
	opacity: 0;
	-moz-box-shadow:    0 2px 25px rgba(0,0,0,0.5);
	-webkit-box-shadow: 0 2px 25px rgba(0,0,0,0.5);
	box-shadow:	        0 2px 25px rgba(0,0,0,0.5);
	-o-transition:      all 0.2s ease;
	-moz-transition:    all 0.2s ease;
	-webkit-transition: all 0.2s ease;
	transition:         all 0.2s ease;
}
.call:hover:before {
	opacity: 1;
}




